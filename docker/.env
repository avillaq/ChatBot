# Configuración global Docker

# Red
NETWORK_NAME=chatbot-p2p-network

# Versión de la imagen
IMAGE_VERSION=latest

# Configuración de logs
LOG_LEVEL=INFO

# Configuración de base de datos
DB_PERSIST=true

# Monitoreo
MONITORING_INTERVAL=30

# Configuración de red P2P
P2P_AUTO_CONNECT=true
P2P_HEARTBEAT_INTERVAL=10